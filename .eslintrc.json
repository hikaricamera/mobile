{
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "operator-linebreak": [2, "before", { "overrides": { "=": "after" } }],
    "implicit-arrow-linebreak": "off",
    "arrow-body-style": "off"
  }
}
