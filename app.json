{
  "expo": {
    "name": "Hikari",
    "slug": "Hikari",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "packagerOpts": {
      "sourceExts": ["ts", "tsx", "js"],
      "transformer": "node_modules/react-native-typescript-transformer/index.js"
    }
  }
}
